<%= form_for @rating do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :story_id %><br />
    <%= f.text_field :story_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p><%= f.submit %></p>
<% end %>
