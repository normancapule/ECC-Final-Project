<%= form_for [@project,@userproject] do |f| %>
  <%= f.error_messages %>
  <strong>Select member</strong></br>
  <% for user in @user %>
    
      <%= check_box_tag "user_ids[]", user.id %>
      <%= user.name %>
      </br>
  <% end %>
  </br>
  <strong><%= f.label :role %></strong> </br>
  
      <%= check_box_tag "user_roles[]", "Owner"%>
      <%= "Owner" %>
      </br>
      <%= check_box_tag "user_roles[]", "Manager"%>
      <%= "Manager" %>
      </br>
      <%= check_box_tag "user_roles[]", "Member"%>
      <%= "Member" %>
      </br>
   
  
  <p><%= f.submit "Add Members" %></p>
<% end %>
