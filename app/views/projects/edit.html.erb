<% title "Edit Project" %>

<%= form_for @project do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :project_name %><br />
    <%= f.text_field :project_name %>
  </p>
  <p>
    <%= f.label :project_description %><br />
    <%= f.text_field :project_description %>
  </p>
  <p><%= f.submit %></p>
<% end %>


<p>
  <%= link_to "Show", @project %> |
  <%= link_to "View All", projects_path %>
</p>
